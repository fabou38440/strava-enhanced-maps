{
  "name": "strava-enhanced-maps",
  "version": "0.10",
  "manifest_version": 2,
  "description": "Strava Enhanced Maps",
  "homepage_url": "http://www.github.com/xavierdeneux",
  "icons": {
    "128": "icons/strava-enhanced-maps-128.png"
  },
  "permissions": [
    "https://www.strava.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.strava.com/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "enhancedMaps.js"
  ]
}
