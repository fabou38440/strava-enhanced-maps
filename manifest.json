{
  "name": "strava-enhanced-maps",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Strava Enhanced Maps",
  "homepage_url": "https://github.com/xavierdeneux/strava-enhanced-maps",
  "icons": {
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://www.strava.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.strava.com/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "enhancedMaps.js",
    "style.css"
  ]
}
